<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.koreait.shopping.menu.MenuMapper">
    <select id="selMenuCategoryList" resultType="BoardCategoryEntity">
        SELECT * FROM t_board_category ORDER BY icategory
    </select>
    <select id="selSubMenuCategoryList" resultType="BoardSubCategoryEntity">
        SELECT * FROM t_board_sub_category ORDER BY isubcategory
    </select>

</mapper>